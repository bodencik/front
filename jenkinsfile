pipeline {
  agent any
  tools {nodejs "node"}
  stages {
    stage('Cloning Git') {
     steps {
       // git 'https://github.com/bodencik/front.git'
       echo 'clonando git'
      }
    }
    stage('build') {
      steps {
        sh 'node -v'
        sh 'npm -v'
        sh 'npm install'
      }
    }
